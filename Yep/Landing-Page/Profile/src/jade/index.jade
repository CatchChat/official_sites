- var name = "Yep"
- var desc = "Meet Genius"
- var keyw = "Yep, App"

- var cdn  = "https://dn-catchinc.qbox.me"

- var iTunesLink = "https://itunes.apple.com/app/id983891256"
- var AndroidApk = "#"

doctype html
html(lang="en")
  head
    title #{name} - #{desc}
    meta(charset='UTF-8')
    meta(name="keywords" content="#{keyw}")
    meta(name='description' content='#{desc}')
    meta(name="viewport" content="width=device-width user-scalable=no initial-scale=1 minimum-scale=1 maximum-scale=1")
    link(rel='shortcut icon' href='/favicon.png' type='image/x-icon') 
    link(href='../css/profile.css' type='text/css' rel='stylesheet')
  





  body
    .wrapper
      .container
        .header
        .main
          .card
            .spiner
            .avatar
            .badge
            .nickname
            .location
            .links
              a(target="_blank").github
              a(target="_blank").dribbble
              a(target="_blank").instagram
              //- a(target="_blank").behance
            .intro
            .skillset.master
            .skillset.learning
        .footer
          .appicon
          .appname Yep
          .appdesc Meet Genius
          .buttons
            a.ios(href="#{iTunesLink}" target="_blank")
            a.android(href="#{AndroidApk}" target="_blank")

    canvas#zodiac





    script(type="application/javascript", src="#{cdn}/zodiac.min.js")
    script(type="application/javascript", src="#{cdn}/jquery-2.1.4.min.js")
    script(type="application/javascript", src="#{cdn}/detect.js")
    script(type="application/javascript", src="/js/profile.js")

